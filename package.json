{
  "name": "next-fullstack-boilerplate",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "24.10.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "shell": "docker compose run --rm node sh",
    "docker:build": "docker build -t next-fullstack-boilerplate:local -f ./docker/dev/Dockerfile .",
    "docker:build-prod": "docker build -t next-fullstack-boilerplate:latest -f ./docker/prod/Dockerfile .",
    "docker:start": "docker compose up next",
    "docker:start-prod": "docker compose up next -d",
    "db:push": "docker compose run --rm node npx prisma db push",
    "db:run-migrations": "docker compose run --rm node npx prisma migrate deploy",
    "db:generate-migration": "docker compose run --rm node npx prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "6.18.0",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@tailwindcss/postcss": "4.1.15",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.38.0",
    "eslint-config-next": "16.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "tailwindcss": "4.1.15",
    "typescript": "5.9.3"
  }
}
