{
  "name": "next-fullstack-boilerplate",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "24.10.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "shell": "docker compose run --rm node sh",
    "docker:build": "docker build -t next-fullstack-boilerplate:local -f ./docker/dev/Dockerfile .",
    "docker:build-prod": "docker build -t next-fullstack-boilerplate:latest -f ./docker/prod/Dockerfile .",
    "docker:start": "docker compose up next",
    "docker:start-prod": "docker compose up next -d",
    "db:push": "docker compose run --rm node npx prisma db push",
    "db:run-migrations": "docker compose run --rm node npx prisma migrate deploy",
    "db:generate-migration": "docker compose run --rm node npx prisma migrate dev"
  },
  "dependencies": {
    "@hookform/resolvers": "5.2.2",
    "@prisma/client": "6.18.0",
    "@radix-ui/react-avatar": "1.1.10",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.7",
    "@radix-ui/react-scroll-area": "1.2.10",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "bcryptjs": "3.0.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-react": "0.548.0",
    "next": "16.0.1",
    "next-auth": "5.0.0-beta.30",
    "next-themes": "0.4.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.65.0",
    "sonner": "2.0.7",
    "tailwind-merge": "3.3.1",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "9.38.0",
    "@tailwindcss/postcss": "4.1.16",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.38.0",
    "eslint-config-next": "16.0.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "tailwindcss": "4.1.16",
    "tsx": "4.20.6",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3"
  }
}
